#ifndef HOJADERUTA_H_
#define HOJADERUTA_H_

typedef struct{

	int dia;
	int mes;
	int anio;

}fechas;


typedef struct{

	int idHoja; //(comienza en 20000)
	int transporteId; //(debe existir) Validar
	float precioViaje;
	float kmsTotales;
	fechas fecha;

}hojaDeRuta;


#endif /* HOJADERUTA_H_ */
