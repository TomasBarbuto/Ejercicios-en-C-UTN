/*
 Ejercicio 5-1:
 Pedir el ingreso de 5 n√∫meros. Mostrarlos y calcular la sumatoria de los mismos.
 */

#include <stdio.h>
#include <stdlib.h>
#include "validaciones.h"
#include "Arrays.h"

int main(void) {
	setbuf(stdout, NULL);

	int arrayNumeros[5];
	int i;
	int acumulador = 0;

	inicializarArrays(arrayNumeros,5);
	sumarArrays(arrayNumeros,5,&acumulador);
	imprimirArray(arrayNumeros, 5);
	printf("La suma de los 5 numeros ingresado es: %d", acumulador);

	return 0;
}
