#ifndef JUGADORES_H_
#define JUGADORES_H_

typedef struct{

	int id;
	char nombre[50];
	char posicion[50];
	short numeroCamiseta;
	int idConfederacion;
	float salario;
	short aniosContrato;
	short isEmpty;

}eJugador;


int inicializarJugadores(eJugador Jugadores[], int longitud);
#endif /* JUGADORES_H_ */
