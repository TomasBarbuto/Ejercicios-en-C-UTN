/*
 Ejercicio 3-1:
 Crear una función que muestre por pantalla el número flotante que recibe como parámetro.
 */

#include <stdio.h>
#include <stdlib.h>
#include "Validaciones.h"

int main(void) {
	setbuf(stdout, NULL);

	float numeroFlotante;

	printf("Ingrese un numero FLOTANTE: ");
	scanf("%f", &numeroFlotante);




	return 0;
}

